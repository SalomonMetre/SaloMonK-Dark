{
  "name": "Harper - Gruvbox Dark Hard (Luminous Teal - No Bold)",
  "type": "dark",
  "colors": {
    "editor.background": "#1d2021",
    "editor.foreground": "#ebdbb2",
    "editor.lineHighlightBackground": "#282828",
    "editorCursor.foreground": "#fabd2f",
    "editor.selectionBackground": "#3c383680",
    "editor.selectionHighlightBackground": "#3c383640",
    "editor.wordHighlightBackground": "#282828",
    "editor.wordHighlightStrongBackground": "#3c3836",
    "activityBar.background": "#1d2021",
    "activityBar.foreground": "#ebdbb2",
    "activityBarBadge.background": "#ebdbb2",
    "activityBarBadge.foreground": "#1d2021",
    "sideBar.background": "#1d2021",
    "sideBar.foreground": "#a89984",
    "sideBarTitle.foreground": "#928374",
    "statusBar.background": "#1d2021",
    "statusBar.foreground": "#a89984",
    "titleBar.activeBackground": "#1d2021",
    "titleBar.activeForeground": "#ebdbb2",
    "editorGroupHeader.tabsBackground": "#1d2021",
    "tab.activeBackground": "#282828",
    "tab.activeForeground": "#ebdbb2",
    "tab.activeBorderTop": "#ebdbb2",
    "tab.inactiveBackground": "#1d2021",
    "tab.inactiveForeground": "#928374"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#928374",
        "fontStyle": ""
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#d5c4a1"
      }
    },
    {
      "name": "Python Docstrings",
      "scope": ["string.quoted.docstring.multi.python", "string.quoted.multi.python"],
      "settings": {
        "foreground": "#d5c4a1",
        "fontStyle": ""
      }
    },
    {
      "name": "Keywords / Storage / Operators",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#95e6cb" 
      }
    },
    {
      "name": "Function Names (White)",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Function Calls",
      "scope": ["meta.function-call"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Types / Classes",
      "scope": ["entity.name.type", "entity.name.class", "support.class", "support.type"],
      "settings": {
        "foreground": "#fabd2f"
      }
    },
    {
      "name": "Variables & Parameters",
      "scope": ["variable", "variable.parameter", "variable.other"],
      "settings": {
        "foreground": "#95e6cb"
      }
    },
    {
      "name": "Numbers & Constants",
      "scope": ["constant.numeric", "constant.language", "support.constant"],
      "settings": {
        "foreground": "#d3869b"
      }
    },
    {
      "name": "Punctuation / Braces / Delimiters",
      "scope": ["punctuation", "meta.brace", "punctuation.separator", "punctuation.section"],
      "settings": {
        "foreground": "#a89984"
      }
    },
    {
      "name": "Attributes / JSON Keys / HTML Tags",
      "scope": ["entity.other.attribute-name", "support.type.property-name", "entity.name.tag"],
      "settings": {
        "foreground": "#a89984"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": ["markup.heading", "markdown.heading"],
      "settings": {
        "foreground": "#ebdbb2"
      }
    }
  ]
}