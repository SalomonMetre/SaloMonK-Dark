{
  "name": "Harper - Gruvbox Dark Hard (Luminous Teal - No Bold) - Deep Black",
  "type": "dark",
  "colors": {
    "editor.background": "#0f0f0f",
    "editor.foreground": "#ebdbb2",
    "editor.lineHighlightBackground": "#1a1a1a",
    "editorCursor.foreground": "#fabd2f",
    "editor.selectionBackground": "#3c383680",
    "editor.selectionHighlightBackground": "#3c383640",
    "editor.wordHighlightBackground": "#1a1a1a",
    "editor.wordHighlightStrongBackground": "#3c3836",
    "activityBar.background": "#0f0f0f",
    "activityBar.foreground": "#ebdbb2",
    "activityBarBadge.background": "#ebdbb2",
    "activityBarBadge.foreground": "#0f0f0f",
    "sideBar.background": "#0f0f0f",
    "sideBar.foreground": "#a89984",
    "sideBarTitle.foreground": "#928374",
    "statusBar.background": "#0f0f0f",
    "statusBar.foreground": "#a89984",
    "titleBar.activeBackground": "#0f0f0f",
    "titleBar.activeForeground": "#ebdbb2",
    "editorGroupHeader.tabsBackground": "#0f0f0f",
    "tab.activeBackground": "#1a1a1a",
    "tab.activeForeground": "#ebdbb2",
    "tab.activeBorderTop": "#ebdbb2",
    "tab.inactiveBackground": "#0f0f0f",
    "tab.inactiveForeground": "#928374"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#928374",
        "fontStyle": ""
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#d5c4a1"
      }
    },
    {
      "name": "Python Docstrings",
      "scope": ["string.quoted.docstring.multi.python", "string.quoted.multi.python"],
      "settings": {
        "foreground": "#d5c4a1",
        "fontStyle": ""
      }
    },
    {
      "name": "Keywords / Storage / Operators",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#95e6cb" 
      }
    },
    {
      "name": "Function Names (White)",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Function Calls",
      "scope": ["meta.function-call"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Types / Classes",
      "scope": ["entity.name.type", "entity.name.class", "support.class", "support.type"],
      "settings": {
        "foreground": "#fabd2f"
      }
    },
    {
      "name": "Variables & Parameters",
      "scope": ["variable", "variable.parameter", "variable.other"],
      "settings": {
        "foreground": "#95e6cb"
      }
    },
    {
      "name": "Numbers & Constants",
      "scope": ["constant.numeric", "constant.language", "support.constant"],
      "settings": {
        "foreground": "#d3869b"
      }
    },
    {
      "name": "Punctuation / Braces / Delimiters",
      "scope": ["punctuation", "meta.brace", "punctuation.separator", "punctuation.section"],
      "settings": {
        "foreground": "#a89984"
      }
    },
    {
      "name": "HTML/XML Tags",
      "scope": [
        "entity.name.tag",
        "entity.name.tag.html",
        "entity.name.tag.xml",
        "meta.tag.structure.any.html",
        "meta.tag.other.html",
        "meta.tag.block.any.html",
        "meta.tag.inline.any.html"
      ],
      "settings": {
        "foreground": "#95e6cb"
      }
    },
    {
      "name": "HTML/XML Tag Punctuation",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.xml",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end"
      ],
      "settings": {
        "foreground": "#a89984"
      }
    },
    {
      "name": "HTML/XML Attributes",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.xml",
        "entity.other.attribute-name.class",
        "entity.other.attribute-name.id"
      ],
      "settings": {
        "foreground": "#fabd2f"
      }
    },
    {
      "name": "HTML/XML Attribute Values",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.quoted.double.xml",
        "string.quoted.single.xml"
      ],
      "settings": {
        "foreground": "#d5c4a1"
      }
    },
    {
      "name": "JSON Keys",
      "scope": [
        "support.type.property-name.json",
        "meta.structure.dictionary.key.json",
        "string.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#95e6cb"
      }
    },
    {
      "name": "JSON String Values",
      "scope": [
        "meta.structure.dictionary.value.json string.quoted.double.json",
        "string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#d5c4a1"
      }
    },
    {
      "name": "JSON Punctuation",
      "scope": [
        "punctuation.support.type.property-name.begin.json",
        "punctuation.support.type.property-name.end.json",
        "punctuation.separator.dictionary.key-value.json",
        "punctuation.separator.dictionary.pair.json",
        "punctuation.separator.array.json"
      ],
      "settings": {
        "foreground": "#a89984"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#95e6cb"
      }
    },
    {
      "name": "CSS Properties",
      "scope": [
        "support.type.property-name.css",
        "meta.property-name.css"
      ],
      "settings": {
        "foreground": "#fabd2f"
      }
    },
    {
      "name": "CSS Property Values",
      "scope": [
        "support.constant.property-value.css",
        "meta.property-value.css"
      ],
      "settings": {
        "foreground": "#d5c4a1"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": ["markup.heading", "markdown.heading"],
      "settings": {
        "foreground": "#ebdbb2"
      }
    }
  ]
}