{
    "name": "SaloMonK Elemental Final v5",
    "type": "dark",
    "colors": {
        "editor.background": "#1e1e1e",
        "editor.foreground": "#C0C0C0",
        "editor.lineHighlightBackground": "#2e2b2b",
        "editor.wordHighlightBackground": "#3C3C30",
        "editorCursor.foreground": "#61ACAB",
        "editor.selectionBackground": "#3C3C30",
        "activityBarBadge.background": "#497f7d",
        "activityBarBadge.foreground": "#ffffff",
        "sideBarTitle.foreground": "#a09a94",
        "sideBar.background": "#1e1e1e",
        "sideBar.foreground": "#a09a94",
        "statusBar.background": "#1e1e1e",
        "statusBar.foreground": "#a09a94",
        "titleBar.activeBackground": "#1e1e1e",
        "titleBar.activeForeground": "#a09a94",
        "editorGroupHeader.tabsBackground": "#1e1e1e",
        "tab.activeBackground": "#1e1e1e",
        "tab.activeForeground": "#ffffff",
        "tab.activeBorderTop": "#61ACAB",
        "tab.inactiveBackground": "#1e1e1e",
        "tab.inactiveForeground": "#808080"
    },
    "tokenColors": [
        {
            "name": "Comment (Neutral Grey Italic)",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#808080"
            }
        },
        {
            "name": "Quoted Strings (Neutral Grey)",
            "scope": [
                "string.quoted"
            ],
            "settings": {
                "foreground": "#808080"
            }
        },
        {
            "name": "Base Source (Silver)",
            "scope": [
                "source"
            ],
            "settings": {
                "foreground": "#C0C0C0"
            }
        },
        {
            "name": "Normal Text/Punctuation (Lighter Grey)",
            "scope": [
                "variable",
                "punctuation",
                "meta.tag",
                "punctuation.definition.tag",
                "punctuation.separator",
                "punctuation.section",
                "meta.brace",
                "meta.delimiter"
            ],
            "settings": {
                "foreground": "#a09a94",
                "fontStyle": ""
            }
        },
        {
            "name": "Keywords & Storage (Midpoint Teal Normal)",
            "scope": [
                "keyword",
                "keyword.control",
                "keyword.other",
                "keyword.operator.new",
                "storage.modifier",
                "storage.type"
            ],
            "settings": {
                "foreground": "#61ACAB",
                "fontStyle": "normal"
            }
        },
        {
            "name": "Classes (Dark Teal Bold)",
            "scope": [
                "entity.name.type",
                "support.class",
                "entity.other.inherited-class",
                "support.type",
                "entity.name.class"
            ],
            "settings": {
                "foreground": "#497f7d",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Functions (Pure White)",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "variable.function",
                "support.function",
                "keyword.other.special-method"
            ],
            "settings": {
                "foreground": "#ffffff",
                "fontStyle": ""
            }
        },
        {
            "name": "Template Strings / Symbols (Midpoint Teal)",
            "scope": [
                "string",
                "string.template",
                "constant.other.symbol",
                "constant.other.key"
            ],
            "settings": {
                "foreground": "#61ACAB"
            }
        },
        {
            "name": "Numbers & Constants (Pure White)",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant",
                "constant.character",
                "constant.escape",
                "keyword.other.unit"
            ],
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Attributes (Light Blue Italic)",
            "scope": [
                "entity.other.attribute-name",
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#96C3E8",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Tags (Teal Normal)",
            "scope": [
                "entity.name.tag",
                "meta.tag.sgml"
            ],
            "settings": {
                "foreground": "#497f7d"
            }
        },
        {
            "name": "JSON Keys (Midpoint Teal)",
            "scope": [
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#61ACAB"
            }
        },
        {
            "name": "Markdown - Heading (White Bold)",
            "scope": [
                "markdown.heading",
                "markup.heading | markup.heading entity.name"
            ],
            "settings": {
                "foreground": "#ffffff",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown - Link (Midpoint Teal)",
            "scope": [
                "string.other.link.title.markdown"
            ],
            "settings": {
                "foreground": "#61ACAB"
            }
        },
        {
            "name": "Invalid (Bold White)",
            "scope": [
                "invalid",
                "invalid.illegal"
            ],
            "settings": {
                "foreground": "#ffffff",
                "fontStyle": "bold"
            }
        }
    ]
}